<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zy.source.mapper.ScheduleMapper">


    <select id="findJokeSchedule" resultType="java.lang.String">
        SELECT value FROM schedule WHERE name = 'joke'
    </select>

    <select id="findHistoryDaySchedule" resultType="java.lang.String">
        SELECT value FROM schedule WHERE name = 'history_day'
    </select>


</mapper>